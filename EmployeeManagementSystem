            Employee emp2 = new Employee("Michaela Jane Mando", 18045, "Engineering Dept.", 45000);
            Employee emp3 = new Employee("Lorie Mae Manait", 85647, "Engineering Dept.", 290000);
            Employee emp4 = new Employee("Frenz Dominic Cartajena", 69420, "Engineering Dept.", 69420);
            
            emp1.displayEmployeeInfo();
            emp2.displayEmployeeInfo();
            emp3.displayEmployeeInfo();
            emp4.displayEmployeeInfo();
    }
    
}
